{
  "expo": {
    "name": "todoList",
    "slug": "todoList",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/trash.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/Logo.png",
      "resizeMode": "auto",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo.png",
        "backgroundColor": "#333"
      }
    },
    "web": {
      "favicon": "./assets/Logo.png"
    }
  }
}